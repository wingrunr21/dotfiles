[user]
	name = Stafford Brunk
  email = wingrunr21@gmail.com
[core]
  excludesfile = ~/.gitignore_global
  preloadIndex = true
[merge]
  tool = opendiff
[diff]
  wordRegex = [^[:space:]]
[format]
  pretty = %Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset
[color]
  ui = auto
  branch = auto
  diff = auto
  status = auto
  interactive = auto
[apply]
  whitespace = fix
[alias]
  gl = log --graph --abbrev-commit --date=relative
  unstage = reset HEAD
[push]
  default = current
[credential]
	helper = osxkeychain
[url "git@heroku.com:"]
  insteadOf = heroku:
[url "git://github.com/"]
  # Read-only
  insteadOf = gh:
[url "git@github.com:"]
  # With write access
  insteadOf = wgh:
[remote "origin"]
	fetch = +refs/pull/*/head:refs/remotes/origin/pr/*

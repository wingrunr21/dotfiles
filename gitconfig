[user]
  name = Stafford Brunk
  email = wingrunr21@gmail.com
[core]
  excludesfile = /Users/wingrunr21/.gitignore_global
  preloadIndex = true
[format]
  pretty = %Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset
[color]
  ui = auto
  branch = auto
  diff = auto
  status = auto
  interactive = auto
[apply]
  whitespace = fix
[alias]
  gl = log --graph --abbrev-commit --date=relative
  unstage = reset HEAD
  pr = "!f() { git fetch origin pull/$1/head:pr-$1 && git checkout pr-$1; }; f"
[push]
  default = current
[credential]
  helper = osxkeychain
[url "git@heroku.com:"]
  insteadOf = heroku:
[url "git://github.com/"]
  # Read-only
  insteadOf = gh:
[url "git@github.com:"]
  # With write access
  insteadOf = wgh:
[difftool]
  prompt = false
[mergetool]
  prompt = false
[commit]
  gpgsign = true
[gpg]
  program = gpg
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
